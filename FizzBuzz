// FizzBuzz Solution
// Dillon Shanks
// 12/09/19

#include<iostream>
using namespace std;
int main()
{
            	//run a loop from 1 to 100
            	for(int i = 1; i <= 100; i++)
            	{
                            	//if i is multiple of both 3 and 5, print FizzBuzz
                            	if((i % 3 == 0) && (i % 5 == 0))
                            	{
                                            	cout<<"FizzBuzz"<<endl;
                            	}
                            	//if i is multiple of 3 print Fizz
                            	else if(i % 3 == 0)
                            	{
                                            	cout<<"Fizz"<<endl;
                            	}
                            	//if i is multiple of 5 print Buzz
                            	else if(i % 5 == 0)
                            	{
                                            	cout<<"Buzz"<<endl;
                            	}
                            	//else just print the number
                            	else
                            	{
                                            	cout<<i<<endl;
                            	}
            	}
            	return 0;
}
